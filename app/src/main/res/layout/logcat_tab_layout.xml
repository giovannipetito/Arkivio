<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <variable
                name="temp"
                type="it.giovanni.arkivio.model.DarkModeModel" />

        <variable
                name="presenter"
                type="it.giovanni.arkivio.presenter.DarkModePresenter" />
    </data>

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <it.giovanni.arkivio.customview.MultiSwipeRefreshLayout
                android:id="@+id/logcat_swipe_refresh_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <RelativeLayout
                    android:id="@+id/logcat_tab_container"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <include
                        android:id="@+id/include_error_smile"
                        layout="@layout/message_error_layout"
                        android:visibility="gone" />

            </RelativeLayout>

        </it.giovanni.arkivio.customview.MultiSwipeRefreshLayout>

    </RelativeLayout>

</layout>